<!DOCTYPE html>
<html>
  <head>
    <title>Portfolioapp</title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    
    <% if flash[:notice] %>
      <%= notice %>
    <% elsif flash[:error] %>
      <%= error %> 
    <% elsif flash[:alert] %>
      <%= alert %>
    <% end %>

  <%= yield %>

  </body>
</html>
