<% content_for :content do %>
  <div class="row">
    <div class= "col-sm-2" >
      <div class="input-group">
        <form action="/search_results">
          <input type="text" class="form-control" placeholder="Search for...", name="search_keywords">
          <span class="input-group-btn">
            <button class="btn btn-primary" type="submit", name="commit">Go</button>
          </span>
        </form>
      </div>
      <ul class="nav nav-pills nav-stacked">
        <li><a href="/wikies">Home</a></li>
        <li><a href="/privatewikies">My wikies</a></li>
        <li><a href="/wikies/new">Create a Wiky</a></li>  
      </li>
      </ul>
    </div>
    <div class= "col-sm-10">
      <%= content_for?(:news_content) ? yield(:news_content) : yield %>
      <% end %>
    </div>
  </div>  
<%= render template: "layouts/application" %>